# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'CocoaPodsMain' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for CocoaPodsMain
  pod 'CocoaPodsModule', :git => 'https://github.com/cy-koki-yamada/CocoaPodsModule.git'

  target 'CocoaPodsMainTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'CocoaPodsMainUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
